<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="columns">
    <sql id="user">
        HEX(U.UUID) AS U_UUID,
        U.NAME AS U_NAME,
        U.CREATED_AT AS U_CREATED_AT
    </sql>
</mapper>